<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1"> {# HERE = REFERENCEMENT #}

    <title>{% block title %}BotMe !{% endblock %}</title>
    {% block stylesheets %}{% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('assets/front/img/favicon.png') }}" />

    <!-- Material Design Icons -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">

    <!-- Bootstrap core CSS -->
    <link href="{{ asset('assets/front/css/bootstrap.min.css') }}" rel="stylesheet">

    <!-- Material Design Bootstrap Compiled -->
    <link href="{{ asset('assets/front/css/mdb.css') }}" rel="stylesheet">

    <!-- Template style -->
    <link href="{{ asset('assets/front/css/bots/dashboard.css') }}" rel="stylesheet">
</head>

<body>
    <!--Navigation-->
    <nav class="navbar navbar-light grey lighten-4 noMargin" role="navigation" id="navTwo">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>

            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-left">
                    <li>
                        <a href="{{ path('dashboard_v_mlp') }}" class="btn waves-effect waves-light" style="margin-right:unset;margin-right:unset;box-shadow: unset;color:black;"
                            data-toggle="tooltip" data-placement="right" title="Tooltip on right">
                            <i class="fa fa-dashboard"></i> Bienvenue sur le panneau de configuration de BotMe
                        </a>

                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <!--Navigation-->
    <div class="bodyStyle">
        {% block body %}{% endblock %} {% block javascripts %}{% endblock %}
    </div>
</body>

<!-- Popper -->
<script type="text/javascript" src="{{ asset('assets/front/js/popper.min.js') }}"></script>

<!-- JQuery -->
<script type="text/javascript" src="{{ asset('assets/front/js/jquery.min.js') }}"></script>

<!-- Bootstrap core JavaScript -->
<script type="text/javascript" src="{{ asset('assets/front/js/bootstrap.min.js') }}"></script>

<!-- Material Design Bootstrap -->
<script type="text/javascript" src="{{ asset('assets/front/js/mdb.js') }}"></script>

</html>

