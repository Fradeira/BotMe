{% extends 'base.html.twig' %}

    {% block body %}
        <!-- Carousel -->
        <div id="carousel-bg" class="carousel carousel-full-bg slide carousel-fade" data-interval="false">

            <!-- Indicators -->
            <ol class="carousel-indicators">
                <li data-target="#carousel-bg" data-slide-to="0" class="active">
                </li>
                <li data-target="#carousel-bg" data-slide-to="1"></li>
                <li data-target="#carousel-bg" data-slide-to="2"></li>
            </ol>

            <!-- Wrapper for slides -->
