                    <div class="homepage-hero-module">
                        <div class="carousel-caption animated fadeIn">
                            <h3>BotMe - Find and get your best I.A</h3>
                            <button type="button" class="btn btn-border-light waves-effect waves-teal" href="#about-me">Learn more</button>
                        </div>
                        <video autoplay loop class="fillWidth">
                            <source src="{{ asset("assets/img/Sail-Away.mp4") }}" type="video/mp4" />

                        </video>
                        <div class="poster hidden">
                            <img src="PATH_TO_JPEG" alt="">
                        </div>
                    </div>
                </div>
                <!-- /.item -->

                <!-- Second slide -->
